{
  "source": {
    "include": ["."],
    "exclude": ["node_modules"]
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recurse": true,
    "destination": "./jsdoc/",
    "readme": "./readme.md",
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "light"
    }
  }
}